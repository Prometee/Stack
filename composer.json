{
    "name": "sylius/stack",
    "description": "The set of e-commerce agnostic PHP components for building your Symfony-based projects.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylius project",
            "homepage": "https://sylius.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Sylius/Stack/contributors"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-kernel": "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "symplify/monorepo-builder": "11.2.*",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\TwigEvent\\": "src/TwigEvent/src/"
        }
    },
    "replace": {
        "sylius/twig-event": "self.version"
    }
}
